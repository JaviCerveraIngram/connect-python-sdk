{
  "logging": {
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": {
      "single-line": {
        "class": "logging.Formatter",
        "datefmt": "%Y-%m-%d %H:%M:%S",
        "format": "%(asctime)s %(levelname)-5s %(module)s.%(funcName)s(%(lineno)d): %(message)s"
      }
    },
    "handlers": {
      "console": {
        "level": "DEBUG",
        "class": "logging.StreamHandler",
        "formatter": "single-line",
        "stream": "ext://sys.stdout"
      }
    },
    "root": {
      "handlers": [
        "console"
      ],
      "level": "ERROR"
    }
  }
}
